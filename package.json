    "hardhat-gas-reporter": "^1.0.4",
    "mocha": "^10.2.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^5.1.2",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "lint": "",
    "lint:fix": "",
    "deploy:client": "cp -r ./artifacts/ ../bqeth-client/src/ ; find ../bqeth-client/src/artifacts/ -name '*.json' -exec sed -i '/.*ytecode.*/d' {} \\;",
    "deploy:decrypter": "cp -r ./artifacts/ ../bqeth-decrypter ; find ../bqeth-decrypter/artifacts/ -name '*.json' -exec sed -i '/.*ytecode.*/d' {} \\;",
    "sidekik": "cp -r ./artifacts/ ../bqeth-client/src/ ; find ../bqeth-client/src/artifacts/ -name '*.json' -exec sed -i '/.*ytecode.*/d' {} \\;",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "start:local": "./node_modules/.bin/hardhat --config hardhat.config.local.js node",
    "-deploy:local": "./node_modules/.bin/hardhat --config hardhat.config.ts --network localhost bqeth-deploy",
    "-deploy:diamond": "./node_modules/.bin/hardhat --config hardhat.config.ts --network localhost diamond",
    "test:local": "./node_modules/.bin/hardhat --config hardhat.config.local.js test",
    "-compile:local": "./node_modules/.bin/hardhat --config hardhat.config.local.js compile"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@symblox/hardhat-abi-gen": "^0.2.0",
    "@tovarishfin/hardhat-yul": "^3.0.5",
    "hardhat-deploy": "^0.11.22",
    "hardhat-tracer": "^1.2.1",
    "node": "^19.3.0",
    "nodejs": "^0.0.0",
    "nvm": "^0.0.4",
    "solium": "^1.2.5",
    "typescript-solidity-merkle-tree": "https://github.com/liamzebedee/typescript-solidity-merkle-tree",
    "web3": "^1.8.0"
  }
}
